CREATE DATABASE IF NOT EXISTS companydb;

USE companydb;

CREATE TABLE employee(
    id INT(11) PRIMARY KEY NOT NULL AUTO_INCREMENT
    name VARCHAR(50) DEFAULT NULL
    salary DECIMAL DEFAULT NULL
    token VARCHAR(150) NOT NULL
);

